#ifndef COMMON_COMMONVERSION_H_
#define COMMON_COMMONVERSION_H_

// std
#include <string>

namespace common
{
namespace version
{
inline int getMajor()
{
  return @MAJOR@;
}

inline int getMinor()
{
  return @MINOR@;
}

inline int getBuild()
{
  return @BUILD@;
}

inline std::string getProjectName()
{
  return "@ProjectName@";
}

inline std::string getText()
{
  return "@ProjectName@ v.@MAJOR@.@MINOR@.@BUILD@";
}

}
}


#endif
